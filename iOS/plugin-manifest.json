{
  "api": {
    "require_startup_execution": false,
    "class_name": "ZappJWPlayerAdapter",
    "modules": []
  },
  "dependency_repository_url": [
    "git@github.com:applicaster/zapp-player-plugin-jw.git"
  ],
  "platform": "ios",
  "author_name": "Roman Karpievich",
  "author_email": "r.karpievich@applicaster.com",
  "manifest_version": "3.11.0",
  "name": "JWPlayer",
  "description": "JW Player Plugin for Applicaster Zapp Platform",
  "type": "player",
  "screen": true,
  "identifier": "JWPlayer-Plugin",
  "ui_builder_support": true,
  "dependency_name": "JWPlayerPlugin",
  "dependency_version": "3.11.0",
  "whitelisted_account_ids": [],
  "min_zapp_sdk": "15.0.0",
  "deprecated_since_zapp_sdk": "",
  "unsupported_since_zapp_sdk": "",
  "react_native": false,
  "custom_configuration_fields": [
    {
      "type": "text",
      "key": "playerKey",
      "tooltip_text": "License key for JWPlayer"
    },
    {
      "type": "text",
      "key": "live_preroll_ad_url",
      "tooltip_text": "Live preroll ad URL"
    },
    {
      "type": "text",
      "key": "vod_preroll_ad_url",
      "tooltip_text": "VOD preroll ad URL"
    },
    {
      "type": "text",
      "key": "vod_midroll_ad_url",
      "tooltip_text": "VOD midroll ad URL"
    },
    {
      "type": "text",
      "key": "vod_midroll_offset",
      "tooltip_text": "VOD midroll ad offset"
    },
    {
      "type": "text",
      "key": "jw_skin_url",
      "tooltip_text": "JW Player skin URL"
    },
    {
      "type": "checkbox",
      "key": "airplay",
      "tooltip_text": "Enable Airplay",
      "default": 0
    },
    {
      "type": "checkbox",
      "key": "Chromecast",
      "tooltip_text": "Enables Chromecast functionality and shows the cast button in the player controls",
      "default": 0
    },
    {
      "type": "text",
      "key": "Custom_Media_Receiver_ID",
      "tooltip_text": "Provide the ID only if the customer has developed a Custom Media Receiver"
    }
  ],
  "hooks": {
    "fields": [
      {
        "group": true,
        "label": "Before Load",
        "folded": true,
        "fields": [
          {
            "key": "preload_plugins",
            "type": "preload_plugins_selector",
            "label": "Select Plugins"
          }
        ]
      }
    ]
  }
}
